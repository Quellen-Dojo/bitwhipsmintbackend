  "license": "ISC",
  "dependencies": {
    "@metaplex/js": "^4.11.2",
    "@solana/spl-token": "^0.3.6",
    "@solana/web3.js": "^1.73.0",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "discord-oauth2": "^2.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "follow-redirects": "^1.15.2",
    "ipfs-http-client": "^56.0.3",
    "mongoose": "^6.8.2",
    "sharp": "^0.31.3",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/follow-redirects": "^1.14.1",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
