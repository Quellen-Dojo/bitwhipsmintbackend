    "discord-oauth2": "^2.10.0",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "follow-redirects": "^1.14.8",
    "ipfs-http-client": "^56.0.3",
    "merge-images": "^2.0.0",
    "mongoose": "^6.0.14",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.7.4"
